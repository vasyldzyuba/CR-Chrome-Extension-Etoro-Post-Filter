{
  "manifest_version": 2,
  "name": "Chrome-Extension Etoro Posts Filter",
  "version": "1.2",
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "matches": [
        "https://www.etoro.com/feed"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
  }
}